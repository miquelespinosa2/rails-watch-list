<div class="container">
<h1 class="text-center mt-4">Your playlists</h1>
  <div class="row mt-4">
    <% @lists.each do |list|%>
     <div class="col-3 card">
      <%= link_to list_path(list) do %>
              <h4 class="card-title"><%= list.name %></h4>
      <% end %>
      </div>
    <% end %>
  </div>
</div>
